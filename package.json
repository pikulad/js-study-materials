{
  "name": "javascript-study-materials",
  "version": "1.0.0",
  "description": "Comprehensive JavaScript study materials covering essential concepts and advanced topics",
  "main": "index.js",
  "scripts": {
    "build:pdf": "marp *.md --pdf --output dist/pdf/",
    "build:html": "marp *.md --html --output dist/html/",
    "build:pptx": "marp *.md --pptx --output dist/pptx/",
    "build:all": "npm run build:pdf && npm run build:html && npm run build:pptx",
    "preview": "marp --server *.md"
  },
  "keywords": [
    "javascript",
    "study",
    "materials",
    "closures",
    "functions",
    "objects",
    "this",
    "hoisting",
    "scopes",
    "variables"
  ],
  "author": "pikulad",
  "license": "MIT",
  "devDependencies": {
    "@marp-team/marp-cli": "^3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikulad/js-study-materials.git"
  },
  "bugs": {
    "url": "https://github.com/pikulad/js-study-materials/issues"
  },
  "homepage": "https://github.com/pikulad/js-study-materials#readme"
}
